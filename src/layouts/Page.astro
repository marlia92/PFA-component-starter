---
import Components from "../components/utils/renderBlock.astro";
import Layout from "./BaseLayout.astro";

type Props = {
  frontmatter: {
    title: string;
    contentBlocks: any[];
  };
};

const props = Astro.props.frontmatter;
---

<Layout {...props}>
  <Components bookshop:live contentBlocks={props.contentBlocks} />
</Layout>
